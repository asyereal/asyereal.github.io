{
	"name": "realedu",
	"homepage": "https://realoss.github.io",
	"productName": "RealEdu",
	"version": "1.2.5",
	"license": "GPL3",
	"description": "Unifying education",
	"author": "realoss <realedu957@gmail.com>",
	"main": "main.js",
	"scripts": {
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"postinstall": "electron-builder install-app-deps",
	"build": {
		"appId": "realedu",
		"dmg": {
			"contents": [
				{
					"x": 110,
					"y": 150
				},
				{
					"x": 240,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"linux": {
			"target": "AppImage",
			"icon": "resources/icon-only.png",
			"category": "Utility"
		},
		"win": {
			"target": "NSIS",
			"icon": "resources/icon-only.png"
		}
	},
	"devDependencies": {
		"electron": "latest",
		"electron-builder": "^24.3.0"
	}
}
